{
  "name": "ui-auto",
  "version": "1.1.1",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "rebuild": "electron-rebuild -f -w node-pty"
  },
  "repository": "http://liangwz@git.legion-tech.net/liangwz/UiAuto.git",
  "keywords": [
    "UiAuto",
    "Electron",
    "rpa"
  ],
  "build": {
    "appId": "com.easyup.uiauto",
    "productName": "UiAuto",
    "copyright": "Copyright Â© 2019 ${author}",
    "win": {
      "icon": "build/icon.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": true,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": false,
      "installerIcon": "./build/icon.ico",
      "uninstallerIcon": "./build/icon.ico",
      "installerHeaderIcon": "./build/icon.ico",
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true,
      "shortcutName": "UiAuto",
      "perMachine": true,
      "include": "./build/script/installer.nsh"
    },
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "extraFiles": [
      {
        "from": "env",
        "to": "env",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "public",
        "to": "public",
        "filter": [
          "pyscript/**/*",
          "runner/**/*",
          "utils/**/*",
          "cron/**/*",
          "base_integration/**/*"
        ]
      },
      {
        "from": "global",
        "to": "global",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "public/node_modules",
        "to": "node_modules",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": ".uiauto",
        "to": ".uiauto",
        "filter": [
          "**/*"
        ]
      }
    ]
  },
  "author": "Juno Leung",
  "license": "Juno",
  "dependencies": {
    "async": "3.1.0",
    "axios": "0.18.0",
    "chokidar": "^3.0.1",
    "decompress": "4.2.0",
    "delay": "4.2.0",
    "express": "4.17.1",
    "fs-extra": "7.0.1",
    "iconv-lite": "0.5.0",
    "ip": "^1.1.5",
    "lodash": "4.17.11",
    "microjob": "0.5.2",
    "mysql2": "^1.6.5",
    "node-machine-id": "1.1.12",
    "node-pty": "0.8.0",
    "nodemailer": "^6.2.1",
    "npm": "6.9.0",
    "path": "0.12.7",
    "progress-stream": "2.0.0",
    "python-shell": "1.0.7",
    "qs": "6.7.0",
    "request": "2.88.0",
    "selenium-webdriver": "4.0.0-alpha.1",
    "sequelize": "^5.9.2",
    "socket.io": "2.3.0",
    "socket.io-client": "2.2.0",
    "unzip": "0.1.11",
    "ws": "7.1.0",
    "xlsx": "0.14.1",
    "xterm": "3.13.0",
    "body-parser": "1.19.0",
    "@fire-dream/sandbox": "1.0.12"
  },
  "devDependencies": {
    "electron": "4.1.1",
    "electron-builder": "^22.6.1"
  }
}
