{
  "name": "ui-auto",
  "version": "2.1.0",
  "description": "2.1.0_Release",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "pack_win": "electron-builder --win",
    "pack_linux": "electron-builder --linux",
    "pack_mac": "electron-builder --mac",
    "dist": "electron-builder",
    "rebuild": "electron-rebuild -f -w node-pty"
  },
  "repository": "https://gitee.com/legion-spider-man/UiAuto.git",
  "keywords": [
    "UiAuto",
    "Electron",
    "rpa"
  ],
  "homepage": "legion.easyup.cn",
  "build": {
    "appId": "com.easyup.uiauto",
    "productName": "UiAuto",
    "copyright": "Copyright Â© 2022 ${author}",
    "asar": true,
    "linux": {
      "target": [
        "zip"
      ],
      "category": "Utility",
      "maintainer": "Legion",
      "executableName": "UiAuto",
      "icon": "icon_256x256.png"
    },
    "win": {
      "icon": "build/icon.ico",
      "requestedExecutionLevel": "highestAvailable",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "mac": {
      "icon": "build/icon.icns"
    },
    "nsis": {
      "oneClick": false,
      "allowElevation": false,
      "allowToChangeInstallationDirectory": false,
      "installerIcon": "./build/icon.ico",
      "uninstallerIcon": "./build/icon.ico",
      "installerHeaderIcon": "./build/icon.ico",
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true,
      "shortcutName": "UiAuto",
      "perMachine": true,
      "include": "./build/script/installer.nsh",
      "artifactName": "${productName} Setup ${version} x64.${ext}"
    },
    "files": [
      "**/*",
      "!**/node_modules/*/{CHANGELOG.md,README.md,README,readme.md,readme}",
      "!**/node_modules/*/{test,__tests__,tests,powered-test,example,examples}",
      "!**/node_modules/*.d.ts",
      "!**/node_modules/.bin",
      "!**/*.{iml,o,hprof,orig,pyc,pyo,rbc,swp,csproj,sln,xproj}",
      "!.editorconfig",
      "!**/._*",
      "!**/{.DS_Store,.git,.hg,.svn,CVS,RCS,SCCS,.gitignore,.gitattributes}",
      "!**/{__pycache__,thumbs.db,.flowconfig,.idea,.vs,.nyc_output}",
      "!**/{appveyor.yml,.travis.yml,circle.yml}",
      "!**/{npm-debug.log,yarn.lock,.yarn-integrity,.yarn-metadata.json}",
      "!.uiauto/**",
      "!env/**",
      "!global/**",
      "!public/**",
      "!redist/**"
    ],
    "extraFiles": [
      {
        "from": "env/webdriver/${platform}",
        "to": "env/webdriver/${platform}",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "env/ffmpeg/${platform}_pack",
        "to": "env/ffmpeg/${platform}",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "env/python/${platform}_pack",
        "to": "env/python/${platform}",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "env/jre/${platform}_pack",
        "to": "env/jre/${platform}",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "public",
        "to": "public",
        "filter": [
          "pyscript/**/*",
          "runner/**/*",
          "utils/**/*",
          "cron/**/*",
          "base_integration/**/*"
        ]
      },
      {
        "from": "global",
        "to": "global",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": ".uiauto",
        "to": ".uiauto",
        "filter": [
          "**/*",
          "!temp/*"
        ]
      },
      {
        "from": "build",
        "to": "build",
        "filter": [
          "**/*"
        ]
      },
      {
        "from": "redist",
        "to": "redist",
        "filter": [
          "**/*"
        ]
      }
    ],
    "directories": {
      "buildResources": "build",
      "output": "dist"
    }
  },
  "author": "Legion",
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "2.0.8",
    "@fire-dream/sandbox": "1.0.12",
    "async": "3.2.3",
    "axios": "0.26.1",
    "body-parser": "1.20.0",
    "decompress": "4.2.0",
    "express": "4.17.3",
    "fkill": "7.2.1",
    "fs-extra": "10.0.1",
    "got": "11.8.2",
    "ip": "1.1.5",
    "lodash": "4.17.21",
    "moment": "2.29.2",
    "node-machine-id": "1.1.12",
    "npminstall": "6.1.0",
    "plist": "3.0.5",
    "python-shell": "3.0.1",
    "readline": "1.3.0",
    "sequelize": "6.18.0",
    "socket.io": "2.4.1",
    "socket.io-client": "2.4.0",
    "sqlite3": "5.0.2",
    "uuid": "8.3.2",
    "ws": "8.5.0"
  },
  "devDependencies": {
    "electron": "18.2.4",
    "electron-builder": "22.14.13"
  }
}
